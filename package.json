{
  "name": "angular-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1700.0",
    "@angular/animations": "^17.0.1",
    "@angular/common": "^17.0.1",
